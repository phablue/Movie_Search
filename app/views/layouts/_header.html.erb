      <ul class="nav navbar-nav">
        <li class="active"><%= link_to "Home", "/" %></li>
        <li>
          <%= link_to "Top 20", '/rank?genre=any&reviewer=rotten%20tomatoes&year=any' %>
        </li>
      <ul>
    </div>
    <%= render 'layouts/account' %>
